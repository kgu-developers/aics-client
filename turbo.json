{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check": {},
    "check:fix": { "cache": false },
    "format": {},
    "format:fix": { "cache": false },
    "lint": {},
    "lint:fix": { "cache": false }
  }
}
